    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch file",
            "presentation": {
                "hidden": false,
                "group": "",
                "order": 1
            },
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}",
        },
        {
            "name": "Launch Package",
            "presentation": {
                "hidden": false,
                "group": "",
                "order": 2
            },
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": ["-r", "examples/sh_run.prod", "-s", "examples/syslog"],
        }
    ]
}